{
    "name": "iiirxs/validation-error-normalizer-bundle",
    "description": "A Symfony bundle to serialize symfony validation form errors or constraint violation lists",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.0 || ^5.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0",
        "symfony/form": "^4.4 || ^5.0",
        "symfony/serializer": "^4.0 || ^5.0"
    },
    "suggest": {
        "symfony/property-access": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nik G",
            "email": "iiirxs@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "IIIRxs\\ValidationErrorNormalizerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IIIRxs\\ValidationErrorNormalizerBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "symfony/validator": "^4.4 || ^5.0"
    }
}
